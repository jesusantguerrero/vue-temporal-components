{
  "name": "vue-temporal-components",
  "version": "0.2.2",
  "main": "dist/vue-temporal-components.es.js",
  "unpkg": "dist/vue-temporal-components.umd.js",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview --port 5050",
    "test": "vitest",
    "test:unit": "vitest",
    "test:component": "cypress open-ct",
    "coverage": "vitest --coverage",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs --fix --ignore-path .gitignore",
    "release": "bumpp --commit --tag --push && npm publish"
  },
  "exports": {
    ".": {
      "require": "./dist/vue-temporal-components.umd.js",
      "import": "./dist/vue-temporal-components.es.js"
    },
    "./*": "./*"
  },
  "dependencies": {
    "@cypress/vue": "^3.1.0",
    "@cypress/webpack-dev-server": "^1.8.4",
    "cypress": "^9.5.3",
    "luxon": "^2.3.1",
    "mitt": "^3.0.0",
    "vue": "^3.2.31"
  },
  "devDependencies": {
    "@cypress/vite-dev-server": "^2.2.2",
    "@rushstack/eslint-patch": "^1.1.0",
    "@testing-library/jest-dom": "^5.12.0",
    "@testing-library/vue": "^6.4.0",
    "@vitejs/plugin-vue": "^2.3.1",
    "@vitejs/plugin-vue-jsx": "^1.3.9",
    "@vitest/ui": "^0.8.2",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/test-utils": "^2.0.0-rc.18",
    "@vueuse/core": "^8.2.5",
    "autoprefixer": "^9.8.6",
    "bumpp": "^7.1.1",
    "c8": "^7.11.0",
    "date-fns": "^2.21.1",
    "eslint": "^8.5.0",
    "eslint-plugin-vue": "^8.2.0",
    "jsdom": "^19.0.0",
    "prettier": "^2.5.1",
    "sass": "^1.26.5",
    "tailwindcss": "^3.0.0",
    "vite": "^2.9.1",
    "vitest": "^0.8.1"
  },
  "description": "Date related components for vue",
  "directories": {
    "test": "tests"
  },
  "files": [
    "dist/*.js",
    "dist/*.css",
    "src/"
  ],
  "keywords": [
    "vuejs",
    "roadmap",
    "temporal"
  ],
  "author": "Jesus Guerrero",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jesusantguerrero/vue-temporal-components.git"
  },
  "bugs": {
    "url": "https://github.com/jesusantguerrero/vue-temporal-components/issues"
  },
  "homepage": "https://github.com/jesusantguerrero/vue-temporal-components#readme"
}
