{
  "name": "vue-temporal-components",
  "version": "0.1.1",
  "main": "dist/vue-temporal-components.common.js",
  "unpkg": "dist/vue-temporal-components.umd.min.js",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview --port 5050",
    "test": "vitest --environment jsdom",
    "test:unit": "vitest --environment jsdom",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs --fix --ignore-path .gitignore",
    "lib": "vue-cli-service build --target lib --name vue-temporal-components ./src/components/index.js"
  },
  "dependencies": {
    "vue": "^3.2.31"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.1.0",
    "@vitejs/plugin-vue": "^2.3.1",
    "@vitejs/plugin-vue-jsx": "^1.3.9",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/test-utils": "^2.0.0-rc.18",
    "eslint": "^8.5.0",
    "eslint-plugin-vue": "^8.2.0",
    "jsdom": "^19.0.0",
    "prettier": "^2.5.1",
    "vite": "^2.9.1",
    "vitest": "^0.8.1",
    "tailwindcss": "^3.0.0",
    "@testing-library/jest-dom": "^5.12.0",
    "@testing-library/vue": "^6.4.0",
    "autoprefixer": "^9.8.6",
    "date-fns": "^2.21.1",
    "sass": "^1.26.5"
  },
  "description": "Date related components for vue",
  "directories": {
    "test": "tests"
  },
  "files": [
    "dist/*.js",
    "dist/*.css"
  ],
  "keywords": [
    "vuejs",
    "roadmap",
    "temporal"
  ],
  "author": "Jesus Guerrero",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jesusantguerrero/vue-temporal-components.git"
  },
  "bugs": {
    "url": "https://github.com/jesusantguerrero/vue-temporal-components/issues"
  },
  "homepage": "https://github.com/jesusantguerrero/vue-temporal-components#readme"
}
