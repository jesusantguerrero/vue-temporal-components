{
  "name": "vue-temporal-components",
  "version": "0.5.0",
  "main": "dist/vue-temporal-components.mjs",
  "unpkg": "dist/vue-temporal-components.umd.js",
  "scripts": {
    "docs:build": "vitepress build docs",
    "build": "vite build",
    "preview": "vite preview --port 5050",
    "test": "vitest",
    "test:unit": "vitest",
    "test:types": "tsc --build tsconfig.json",
    "coverage": "vitest --coverage",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs --fix --ignore-path .gitignore",
    "release": "npm run build && bumpp --commit --tag --push && npm publish",
    "dev": "vitepress dev docs",
    "docs:serve": "vitepress serve docs"
  },
  "exports": {
    ".": {
      "require": "./dist/vue-temporal-components.umd.js",
      "import": "./dist/vue-temporal-components.mjs"
    },
    "./*": "./*",
    "./timer": "./src/components/Timer/index.vue"
  },
  "peerDependencies": {
    "@vueuse/core": "^10.0.0"
  },
  "dependencies": {
    "@interactjs/actions": "^1.10.17",
    "@interactjs/auto-start": "^1.10.17",
    "@interactjs/dev-tools": "^1.10.17",
    "@interactjs/interact": "^1.10.17",
    "@interactjs/modifiers": "^1.10.17",
    "@types/lodash-es": "^4.17.8",
    "lodash-es": "^4.17.21",
    "luxon": "^3.3.0",
    "mathjs": "^11.9.1",
    "mitt": "^3.0.1",
    "rrule": "^2.7.2",
    "vueuse-temporals": "^1.4.3"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.3.2",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/user-event": "^14.4.3",
    "@testing-library/vue": "^7.0.0",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vitejs/plugin-vue-jsx": "^3.0.1",
    "@vitest/coverage-c8": "^0.28.5",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/test-utils": "^2.4.1",
    "@vueuse/core": "^9.13.0",
    "autoprefixer": "^10.4.14",
    "bumpp": "^8.2.1",
    "c8": "^7.14.0",
    "date-fns": "^2.30.0",
    "eslint": "^8.46.0",
    "eslint-plugin-vue": "^9.15.1",
    "jsdom": "^21.1.2",
    "prettier": "^2.8.8",
    "sass": "^1.64.1",
    "tailwindcss": "^3.3.3",
    "typescript": "^4.9.5",
    "vite": "^4.4.7",
    "vitepress": "^1.0.0-draft.8",
    "vitest": "^0.28.5",
    "vue": "^3.3.4"
  },
  "description": "Date related components for vue",
  "directories": {
    "test": "tests"
  },
  "files": [
    "dist/*.js",
    "dist/*.css",
    "src/*"
  ],
  "keywords": [
    "vuejs",
    "roadmap",
    "temporal"
  ],
  "author": "Jesus Guerrero",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jesusantguerrero/vue-temporal-components.git"
  },
  "bugs": {
    "url": "https://github.com/jesusantguerrero/vue-temporal-components/issues"
  },
  "homepage": "https://github.com/jesusantguerrero/vue-temporal-components#readme"
}
