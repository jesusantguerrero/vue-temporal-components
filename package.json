{
  "name": "vue-temporal-components",
  "version": "1.1.0",
  "main": "dist/vue-temporal-components.mjs",
  "unpkg": "dist/vue-temporal-components.umd.js",
  "scripts": {
    "docs:build": "vitepress build docs",
    "build": "vite build",
    "preview": "vite preview --port 5050",
    "test": "vitest",
    "test:unit": "vitest",
    "test:types": "tsc --build tsconfig.json",
    "coverage": "vitest --coverage",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs --fix --ignore-path .gitignore",
    "release": "npm run build && bumpp --commit --tag --push && npm publish",
    "dev": "vitepress dev docs",
    "docs:serve": "vitepress serve docs"
  },
  "exports": {
    ".": {
      "require": "./dist/vue-temporal-components.umd.js",
      "import": "./dist/vue-temporal-components.mjs"
    },
    "./*": "./*",
    "./timer": "./src/components/Timer/index.vue"
  },
  "peerDependencies": {
    "@vueuse/core": "^11.0.0"
  },
  "dependencies": {
    "@interactjs/actions": "^1.10.27",
    "@interactjs/auto-start": "^1.10.27",
    "@interactjs/dev-tools": "^1.10.27",
    "@interactjs/interact": "^1.10.27",
    "@interactjs/modifiers": "^1.10.27",
    "@types/lodash-es": "^4.17.12",
    "interactjs": "^1.10.27",
    "lodash-es": "^4.17.21",
    "luxon": "^3.5.0",
    "mathjs": "^13.2.0",
    "mitt": "^3.0.1",
    "rrule": "^2.8.1",
    "vueuse-temporals": "^1.6.0"
  },
  "devDependencies": {
    "@interactjs/types": "^1.10.27",
    "@rushstack/eslint-patch": "^1.10.4",
    "@testing-library/jest-dom": "^6.5.0",
    "@testing-library/user-event": "^14.5.2",
    "@testing-library/vue": "^8.1.0",
    "@vitejs/plugin-vue": "^4.6.2",
    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "@vitest/coverage-v8": "^2.1.3",
    "@vue/eslint-config-prettier": "^10.0.0",
    "@vue/test-utils": "^2.4.6",
    "@vueuse/core": "^10.11.1",
    "autoprefixer": "^10.4.20",
    "bumpp": "^9.7.1",
    "c8": "^7.14.0",
    "date-fns": "^4.1.0",
    "eslint": "^8.57.1",
    "eslint-plugin-vue": "^9.29.0",
    "jsdom": "^25.0.1",
    "prettier": "^3.3.3",
    "sass": "^1.79.5",
    "tailwindcss": "^3.4.13",
    "typescript": "^5.6.3",
    "vite": "^4.5.5",
    "vitepress": "^1.4.1",
    "vitest": "^2.1.3",
    "vue": "^3.5.12"
  },
  "description": "Date related components for vue",
  "directories": {
    "test": "tests"
  },
  "files": [
    "dist/*.js",
    "dist/*.css",
    "src/*"
  ],
  "keywords": [
    "vuejs",
    "roadmap",
    "temporal"
  ],
  "author": "Jesus Guerrero",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jesusantguerrero/vue-temporal-components.git"
  },
  "bugs": {
    "url": "https://github.com/jesusantguerrero/vue-temporal-components/issues"
  },
  "homepage": "https://github.com/jesusantguerrero/vue-temporal-components#readme"
}
