{
  "name": "vue-temporal-components",
  "version": "0.4.5",
  "main": "dist/vue-temporal-components.mjs",
  "unpkg": "dist/vue-temporal-components.umd.js",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview --port 5050",
    "test": "vitest",
    "test:unit": "vitest",
    "test:component": "cypress open-ct",
    "test:e2e": "cypress open",
    "test:types": "tsc --build tsconfig.json",
    "coverage": "vitest --coverage",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs --fix --ignore-path .gitignore",
    "release": "npm run build && bumpp --commit --tag --push && npm publish",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:serve": "vitepress serve docs"
  },
  "exports": {
    ".": {
      "require": "./dist/vue-temporal-components.umd.js",
      "import": "./dist/vue-temporal-components.mjs"
    },
    "./*": "./*",
    "./timer": "./src/components/Timer/index.vue"
  },
  "peerDependencies": {
    "@vueuse/core": "^9.1.1"
  },
  "dependencies": {
    "@interactjs/actions": "^1.10.17",
    "@interactjs/auto-start": "^1.10.17",
    "@interactjs/dev-tools": "^1.10.17",
    "@interactjs/interact": "^1.10.17",
    "@interactjs/modifiers": "^1.10.17",
    "@types/lodash-es": "^4.17.6",
    "lodash-es": "^4.17.21",
    "luxon": "^3.2.1",
    "mathjs": "^11.5.0",
    "mitt": "^3.0.0",
    "rrule": "^2.7.1",
    "vueuse-temporals": "^1.4.3"
  },
  "devDependencies": {
    "@cypress/vite-dev-server": "^5.0.2",
    "@cypress/vue": "^5.0.3",
    "@rushstack/eslint-patch": "^1.2.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/user-event": "^14.4.3",
    "@testing-library/vue": "^6.6.1",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vitejs/plugin-vue-jsx": "^3.0.0",
    "@vitest/coverage-c8": "^0.27.1",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/test-utils": "^2.2.7",
    "@vueuse/core": "^9.10.0",
    "autoprefixer": "^10.4.13",
    "bumpp": "^8.2.1",
    "c8": "^7.12.0",
    "cypress": "^12.3.0",
    "date-fns": "^2.29.3",
    "eslint": "^8.31.0",
    "eslint-plugin-vue": "^9.9.0",
    "jsdom": "^21.0.0",
    "prettier": "^2.8.3",
    "sass": "^1.57.1",
    "tailwindcss": "^3.2.4",
    "typescript": "^4.5.4",
    "vite": "^4.0.4",
    "vitepress": "^1.0.0-draft.8",
    "vitest": "^0.27.1",
    "vue": "^3.2.45"
  },
  "description": "Date related components for vue",
  "directories": {
    "test": "tests"
  },
  "files": [
    "dist/*.js",
    "dist/*.css",
    "src/*"
  ],
  "keywords": [
    "vuejs",
    "roadmap",
    "temporal"
  ],
  "author": "Jesus Guerrero",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jesusantguerrero/vue-temporal-components.git"
  },
  "bugs": {
    "url": "https://github.com/jesusantguerrero/vue-temporal-components/issues"
  },
  "homepage": "https://github.com/jesusantguerrero/vue-temporal-components#readme"
}
